[run]
source = .
omit = 
    */venv/*
    */env/*
    */migrations/*
    */settings/*
    */wsgi.py
    */asgi.py
    manage.py
    */tests/*
    */test_*.py
    */__pycache__/*
    */static/*
    */media/*
    .github/*
    logs/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml